{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "864899839841.dkr.ecr.us-east-2.amazonaws.com/learnjenkinsapp:1.0.106",
            "portMappings": [{
                "name": "ngnix-80-tip",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": ".25 vCPU",
    "executionRoleArn": "arn:aws:iam::864899839841:role/ecsTaskExecutionRole"
}